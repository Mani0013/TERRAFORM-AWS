terraform {
  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 6.0"
    }
  }
}

# Configure the AWS Provider
provider "aws" {
  region = "ap-south-1"
}

# Create s3 bucket
resource "aws_s3_bucket" "example" {
  bucket = "terraform-practice-s3-manideep"

  tags = {
    Name        = "My bucket"
    Environment = "Dev"
  }
}